
<div class="movies index">


	<div class="row" summary="List list" style='margin-top:10px;'>
    <% @movies.each do |movieL| %>
    <% plakat = movieL.movie.poster %>
      <% if plakat.blank? %>
          <% plakat = '/assets/noposter.png' %>
      <% end %>
      <div class="col-md-3 shadow"  style='height:255px;width:170px;margin:10px'>
        <div class="box overlay red">
          <img src="<%= plakat %>" width="170px">

        </div>
      </div>
    <% end %>
  </div>
</div>